# Analytics Configuration
# Copy this file to .env and fill in your values

# Analytics Provider
# Options: 'ga4' | 'plausible' | 'fathom' | 'umami' | 'none'
PUBLIC_ANALYTICS_PROVIDER=none

# Tracking ID (required for most providers)
# GA4: G-XXXXXXXXXX
# Plausible: (not needed, uses domain)
# Fathom: XXXXXXXX
# Umami: xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
PUBLIC_ANALYTICS_ID=

# Domain (for Plausible)
PUBLIC_ANALYTICS_DOMAIN=carlosgarcia.works

# Umami Script URL (if self-hosted)
# PUBLIC_UMAMI_SCRIPT_URL=https://your-umami-instance.com/script.js
