---
import BaseLayout from '../layouts/base-layout.astro';
import Header from '../components/layout/header.astro';
import Footer from '../components/layout/footer.astro';
import DotBackground from '../components/ui/dot-background.astro';
import ComingSoonSection from '../components/sections/coming-soon-section.astro';
---

<BaseLayout 
	title="Vult Studio | Carlos Garcia | Client Work & Collaborations" 
	description="Professional studio work, client projects, and collaborative endeavors by Carlos Garcia. Pushing creative boundaries through commercial and artistic partnerships."
	keywords="Studio Work, Client Projects, Commercial 3D, Collaborations, Professional Work, 3D Production, Creative Partnerships"
>
<Header />
	<main class="min-h-screen w-full bg-background text-foreground">
		<DotBackground />
		
		<div class="relative z-10">
			<ComingSoonSection 
				title="Vult" 
				description="Studio work, client projects, and collaborative endeavors pushing creative boundaries. From concept to completion, each project represents a commitment to craft and creative problem-solving."
			/>
		</div>
	</main>
<Footer />
</BaseLayout>
